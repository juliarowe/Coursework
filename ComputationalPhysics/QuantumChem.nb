Cell["\<\
TolueneDelocEnergy takes in the list of eigenvalues and eigenvectors \
calculated above and returns the delocalization energy between toluene and \
six isolated double bonds.\
\>", "Text",
 CellChangeTimes->{{3.6349481774420547`*^9, 3.6349482233615847`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"TolueneDelocEnergy", "[", "eigenList_", "]"}], ":=", 
   RowBox[{"(", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
      RowBox[{
      "Calculate", " ", "energy", " ", "of", " ", "an", " ", "isolated", " ", 
       "carbon"}], "-", 
      RowBox[{"carbon", " ", "double", " ", "bond"}]}], "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"ethylene", "=", 
      RowBox[{"(", GridBox[{
         {"\[Alpha]", "\[Beta]"},
         {"\[Beta]", "\[Alpha]"}
        }], ")"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"ethEigen", "=", 
      RowBox[{"Eigensystem", "[", "ethylene", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"ethVals", "=", 
      RowBox[{
      "ethEigen", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"Clear", "[", 
      RowBox[{"\[Alpha]", ",", " ", "\[Beta]"}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"ethEnergy", "=", 
      RowBox[{"Block", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"\[Alpha]", "=", 
           RowBox[{"-", "1"}]}], ",", " ", 
          RowBox[{"\[Beta]", "=", 
           RowBox[{"-", "1"}]}]}], "}"}], ",", 
        RowBox[{
        "ethVals", "\[LeftDoubleBracket]", "All", "\[RightDoubleBracket]"}]}],
        "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"ethyleneEnergy", "=", 
      RowBox[{"Min", "[", "ethEnergy", "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "Calculate", " ", "the", " ", "energy", " ", "of", " ", "the", " ", 
       "delocalized", " ", "electrons", " ", "in", " ", "the", " ", 
       "molecule"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"numElectrons", "=", "7"}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{"number", " ", "of", " ", "carbon", " ", "atoms"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"eigenValues", "=", 
      RowBox[{
      "eigenList", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "puts", " ", "eigenvalues", " ", "in", " ", "ascending", " ", "order"}],
       "*)"}], "\[IndentingNewLine]", 
     RowBox[{"numericEigenValues", "=", 
      RowBox[{"Sort", "[", 
       RowBox[{"Map", "[", 
        RowBox[{"N", ",", 
         RowBox[{"Block", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"\[Alpha]", "=", 
              RowBox[{"-", "1"}]}], ",", 
             RowBox[{"\[Beta]", "=", 
              RowBox[{"-", "1"}]}]}], "}"}], ",", "eigenValues"}], "]"}]}], 
        "]"}], "]"}]}], ";", " ", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "fill", " ", "the", " ", "energy", " ", "levels", " ", "starting", " ", 
       "with", " ", "the", " ", "lowest"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"moleculeEnergy", "=", 
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{"2", "*", 
         RowBox[{
         "numericEigenValues", "\[LeftDoubleBracket]", "i", 
          "\[RightDoubleBracket]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", 
          RowBox[{"Floor", "[", 
           RowBox[{"numElectrons", "/", "2"}], "]"}]}], "}"}]}], "]"}]}], ";",
      "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"in", " ", "case", " ", 
       RowBox[{"there", "'"}], "s", " ", "an", " ", "odd", " ", "number", " ",
        "of", " ", 
       RowBox[{"carbons", "/", "electrons"}]}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Mod", "[", 
         RowBox[{"numElectrons", ",", "2"}], "]"}], "\[Equal]", "1"}], ",", 
       RowBox[{"moleculeEnergy", "=", 
        RowBox[{"moleculeEnergy", "+", 
         RowBox[{"numericEigenValues", "\[LeftDoubleBracket]", 
          RowBox[{"Ceiling", "[", 
           RowBox[{"numElectrons", "/", "2"}], "]"}], 
          "\[RightDoubleBracket]"}]}]}], ","}], "]"}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"numDblBonds", "=", "6"}], ";", "\[IndentingNewLine]", 
     RowBox[{"dblBondEnergy", "=", 
      RowBox[{"ethyleneEnergy", " ", "numDblBonds"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "The", " ", "difference", " ", "between", " ", "the", " ", "molecule", 
       " ", "and", " ", "the", " ", "isolated", " ", "double", " ", "bonds", 
       " ", "is", " ", "the", " ", "delocalization", " ", "energy"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"Return", "[", 
      RowBox[{"moleculeEnergy", "-", "dblBondEnergy"}], "]"}], ";"}], 
    "\[IndentingNewLine]", ")"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.6349066771067257`*^9, 3.634906756304624*^9}}],

