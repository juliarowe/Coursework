Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"delocEnergy", "[", 
    RowBox[{"eigenList_", ",", 
     RowBox[{"molecule_", "?", "StringQ"}]}], "]"}], ":=", 
   RowBox[{"(", "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{
      RowBox[{
      "Calculate", " ", "energy", " ", "of", " ", "an", " ", "isolated", " ", 
       "carbon"}], "-", 
      RowBox[{"carbon", " ", "double", " ", "bond"}]}], " ", "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"ethylene", "=", 
      RowBox[{"(", GridBox[{
         {"\[Alpha]", "\[Beta]"},
         {"\[Beta]", "\[Alpha]"}
        }], ")"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"ethEigen", "=", 
      RowBox[{"Eigensystem", "[", "ethylene", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"ethVals", "=", 
      RowBox[{
      "ethEigen", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"Clear", "[", 
      RowBox[{"\[Alpha]", ",", " ", "\[Beta]"}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"ethEnergy", "=", 
      RowBox[{"Block", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"\[Alpha]", "=", 
           RowBox[{"-", "1"}]}], ",", " ", 
          RowBox[{"\[Beta]", "=", 
           RowBox[{"-", "1"}]}]}], "}"}], ",", 
        RowBox[{
        "ethVals", "\[LeftDoubleBracket]", "All", "\[RightDoubleBracket]"}]}],
        "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"ethyleneEnergy", "=", 
      RowBox[{"Min", "[", "ethEnergy", "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Calculate", " ", "the", " ", "energy", " ", "of", " ", "the", " ", 
       "delocalized", " ", "electrons", " ", "in", " ", "the", " ", 
       "molecule"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"tally", "=", 
      RowBox[{"ChemicalData", "[", 
       RowBox[{"molecule", ",", "\"\<ElementTally\>\""}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"numElectrons", "=", 
      RowBox[{
       RowBox[{
       "tally", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
       "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{"number", " ", "of", " ", "carbon", " ", "atoms"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"eigenValues", "=", 
      RowBox[{
      "eigenList", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}]}], 
     ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Put", " ", "eigenvalues", " ", "in", " ", "ascending", " ", "order"}], 
      " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"numericEigenValues", "=", 
      RowBox[{"Sort", "[", 
       RowBox[{"Map", "[", 
        RowBox[{"N", ",", 
         RowBox[{"Block", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"\[Alpha]", "=", 
              RowBox[{"-", "1"}]}], ",", 
             RowBox[{"\[Beta]", "=", 
              RowBox[{"-", "1"}]}]}], "}"}], ",", "eigenValues"}], "]"}]}], 
        "]"}], "]"}]}], ";", " ", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Fill", " ", "the", " ", "energy", " ", "levels", " ", "starting", " ", 
       "with", " ", "the", " ", "lowest"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"moleculeEnergy", "=", 
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{"2", "*", 
         RowBox[{
         "numericEigenValues", "\[LeftDoubleBracket]", "i", 
          "\[RightDoubleBracket]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", 
          RowBox[{"Floor", "[", 
           RowBox[{"numElectrons", "/", "2"}], "]"}]}], "}"}]}], "]"}]}], ";",
      "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"If", " ", "an", " ", "odd", " ", "number", " ", "of", " ", 
       RowBox[{"carbons", "/", "electrons"}], " ", "add", " ", "the", " ", 
       "last", " ", 
       RowBox[{"one", "'"}], "s", " ", "energy"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Mod", "[", 
         RowBox[{"numElectrons", ",", "2"}], "]"}], "\[Equal]", "1"}], ",", 
       RowBox[{"moleculeEnergy", "=", 
        RowBox[{"moleculeEnergy", "+", 
         RowBox[{"numericEigenValues", "\[LeftDoubleBracket]", 
          RowBox[{"Ceiling", "[", 
           RowBox[{"numElectrons", "/", "2"}], "]"}], 
          "\[RightDoubleBracket]"}]}]}], ","}], "]"}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"bonds", "=", 
      RowBox[{"ChemicalData", "[", 
       RowBox[{"molecule", ",", "\"\<BondTally\>\""}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"numDblBonds", "=", 
      RowBox[{
       RowBox[{
       "bonds", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
       "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"dblBondEnergy", "=", 
      RowBox[{"ethyleneEnergy", " ", "numDblBonds"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "The", " ", "difference", " ", "between", " ", "the", " ", "molecule", 
       " ", "and", " ", "the", " ", "isolated", " ", "double", " ", "bonds", 
       " ", "is", " ", "the", " ", "delocalization", " ", "energy"}], " ", 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"Return", "[", 
      RowBox[{"moleculeEnergy", "-", "dblBondEnergy"}], "]"}], ";"}], 
    "\[IndentingNewLine]", ")"}]}], ";"}]], "Input",
