Cell["\<\
TolueneEffectiveCharge takes in the list of eigenpairs and the label of a \
certain atom and returns the effective charge density at that atom.\
\>", "Text",
 CellChangeTimes->{{3.634948357028573*^9, 3.6349483876588373`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"TolueneEffectiveCharge", "[", 
    RowBox[{"eigenList_", ",", "atomNumber_"}], "]"}], ":=", 
   RowBox[{"(", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
     "Calculate", " ", "the", " ", "charge", " ", "density", " ", "of", " ", 
      "the", " ", "delocalized", " ", "electrons", " ", "in", " ", "the", " ",
       "molecule"}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"numElectrons", "=", "7"}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{"number", " ", "of", " ", "carbon", " ", "atoms"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"eigenVector", "=", 
      RowBox[{
       RowBox[{
        RowBox[{
        "eigenList", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], 
        "\[LeftDoubleBracket]", "atomNumber", "\[RightDoubleBracket]"}], 
       "\[LeftDoubleBracket]", "All", "\[RightDoubleBracket]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
       "fill", " ", "the", " ", "energy", " ", "levels", " ", "starting", " ",
         "with", " ", "the", " ", "lowest"}], ",", " ", 
       RowBox[{
       "to", " ", "the", " ", "highest", " ", "occupied", " ", "pi", " ", 
        "molecular", " ", "orbital"}]}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"Block", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"\[Alpha]", "=", 
          RowBox[{"-", "1"}]}], ",", " ", 
         RowBox[{"\[Beta]", "=", 
          RowBox[{"-", "1"}]}]}], "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"charge", "=", 
         RowBox[{"Sum", "[", 
          RowBox[{
           RowBox[{"2", "*", 
            SuperscriptBox[
             RowBox[{
             "eigenVector", "\[LeftDoubleBracket]", "i", 
              "\[RightDoubleBracket]"}], "2"]}], ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", 
             RowBox[{"Floor", "[", 
              RowBox[{"numElectrons", "/", "2"}], "]"}]}], "}"}]}], "]"}]}], 
        ";", "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Mod", "[", 
            RowBox[{"numElectrons", ",", "2"}], "]"}], "\[Equal]", "1"}], ",", 
          RowBox[{"charge", "=", 
           RowBox[{"charge", "+", 
            RowBox[{"eigenVector", "\[LeftDoubleBracket]", 
             RowBox[{"Ceiling", "[", 
              RowBox[{"numElectrons", "/", "2"}], "]"}], 
             "\[RightDoubleBracket]"}]}]}]}], "]"}], ";"}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "charge", "]"}], ";"}], "\[IndentingNewLine]", 
    ")"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.6349068107361593`*^9, 3.634906847564701*^9}, 
