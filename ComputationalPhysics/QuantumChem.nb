Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"TolueneBondOrder", "[", 
    RowBox[{"eigenList_", ",", "a1_", ",", "a2_"}], "]"}], ":=", 
   RowBox[{"(", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
     "Calculate", " ", "the", " ", "charge", " ", "density", " ", "of", " ", 
      "the", " ", "delocalized", " ", "electrons", " ", "in", " ", "the", " ",
       "molecule"}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"numElectrons", "=", "7"}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{"number", " ", "of", " ", "carbon", " ", "atoms"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"eigenVectors", "=", 
      RowBox[{
      "eigenList", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}], 
     ";", " ", "\[IndentingNewLine]", 
     RowBox[{"Block", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"\[Alpha]", "=", 
          RowBox[{"-", "1"}]}], ",", 
         RowBox[{"\[Beta]", "=", 
          RowBox[{"-", "1"}]}]}], "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"atom1coeffs", "=", 
         RowBox[{
          RowBox[{
           RowBox[{
           "eigenList", "\[LeftDoubleBracket]", "2", 
            "\[RightDoubleBracket]"}], "\[LeftDoubleBracket]", "a1", 
           "\[RightDoubleBracket]"}], "\[LeftDoubleBracket]", "All", 
          "\[RightDoubleBracket]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"atom2coeffs", "=", 
         RowBox[{
          RowBox[{
           RowBox[{
           "eigenList", "\[LeftDoubleBracket]", "2", 
            "\[RightDoubleBracket]"}], "\[LeftDoubleBracket]", "a2", 
           "\[RightDoubleBracket]"}], "\[LeftDoubleBracket]", "All", 
          "\[RightDoubleBracket]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{
         "calculate", " ", "order", " ", "for", " ", "a", " ", "bond", " ", 
