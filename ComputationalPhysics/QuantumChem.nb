 RowBox[{
  RowBox[{
   RowBox[{"generateMoleculeGraph", "[", 
    RowBox[{"molecule_", "?", "StringQ"}], "]"}], ":=", 
   RowBox[{"(", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"carbonLocations", "=", 
      RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"tmpCount", "=", "0"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Map", "[", 
      RowBox[{
       RowBox[{"Function", "[", 
        RowBox[{
         RowBox[{"{", "atom", "}"}], ",", 
         RowBox[{
          RowBox[{"tmpCount", "++"}], ";", "\[IndentingNewLine]", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"atom", "\[Equal]", "\"\<C\>\""}], ",", 
            RowBox[{"AppendTo", "[", 
             RowBox[{"carbonLocations", ",", "tmpCount"}], "]"}]}], "]"}]}]}],
         "]"}], ",", " ", 
       RowBox[{"(*", " ", 
        RowBox[{"Append", " ", "if", " ", "Carbon"}], " ", "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{"ChemicalData", "[", 
        RowBox[{"molecule", ",", "\"\<VertexTypes\>\""}], "]"}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "If", " ", "the", " ", "molecule", " ", "is", " ", 
