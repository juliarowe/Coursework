Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"generateHuckel", "[", 
    RowBox[{"molecule_", "?", "StringQ"}], "]"}], ":=", 
   RowBox[{"(", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"carbonLocations", "=", 
      RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"tmpCount", "=", "0"}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"Append", " ", "carbon", " ", "locations"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"Map", "[", 
      RowBox[{
       RowBox[{"Function", "[", 
        RowBox[{
         RowBox[{"{", "atom", "}"}], ",", 
         RowBox[{
          RowBox[{"tmpCount", "++"}], ";", "\[IndentingNewLine]", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"atom", "\[Equal]", "\"\<C\>\""}], ",", " ", 
            RowBox[{"AppendTo", "[", 
             RowBox[{"carbonLocations", ",", " ", "tmpCount"}], "]"}]}], 
           "]"}]}]}], "]"}], ",", " ", 
       RowBox[{"(*", " ", 
        RowBox[{"exclude", " ", "hydrogens"}], " ", "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{"ChemicalData", "[", 
        RowBox[{"molecule", ",", "\"\<VertexTypes\>\""}], "]"}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"tmpMatrix", "=", 
      RowBox[{"AdjacencyMatrix", "[", 
       RowBox[{"Subgraph", "[", 
        RowBox[{
         RowBox[{"Graph", "[", 
          RowBox[{"ChemicalData", "[", 
           RowBox[{"molecule", ",", "\"\<EdgeRules\>\""}], "]"}], "]"}], ",", 
         "carbonLocations"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
       "ChemicalData", " ", "only", " ", "returns", " ", "the", " ", "upper", 
        " ", "half", " ", "of", " ", "the", " ", 
        RowBox[{"matrix", ".", " ", "To"}], " ", "represent", " ", "it", " ", 
        "as", " ", "a", " ", "Huckel"}], ",", "  ", 
       RowBox[{
        RowBox[{
        "we", " ", "put", " ", "\[Alpha]", " ", "in", " ", "the", " ", 
         "diagonal", " ", "entries", " ", "and", " ", "\[Beta]", " ", "to", 
         " ", "the", " ", "non"}], "-", 
        RowBox[{
        "zero", " ", "entries", " ", "on", " ", "the", " ", "top", " ", "and",
          " ", 
         RowBox[{"bottom", "."}]}]}]}], "\[IndentingNewLine]", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"huckel", "=", 
      RowBox[{
       RowBox[{"\[Beta]", 
        RowBox[{"(", 
         RowBox[{"tmpMatrix", "+", 
          RowBox[{"Transpose", "[", "tmpMatrix", "]"}]}], ")"}]}], "+", 
       RowBox[{"\[Alpha]", " ", 
        RowBox[{"IdentityMatrix", "[", 
         RowBox[{"Length", "[", "tmpMatrix", "]"}], "]"}]}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "huckel", "]"}]}], "\[IndentingNewLine]", ")"}]}],
   ";"}]], "Input",
 CellGroupingRules->{GroupTogetherGrouping, 10000.},
